{
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "APP_ENV=production vite build",
    "dev-admin": "vite --config vite.config.wp-admin.js",
    "build-admin": "APP_ENV=production vite build --config vite.config.wp-admin.js",
    "build-editor": "vite build --config vite.config.wp-editor.js",
    "build-watch-editor": "vite build --config vite.config.wp-editor.js --watch",
    "build-all": "npm run build && npm run build-admin && npm run build-editor",
    "upgrade": "npx npm-check-updates -u && npm install"
  },
  "dependencies": {
    "gsap": "^3.6.1",
    "lodash-es": "^4.17.21",
    "ohmyfetch": "^0.2.0",
    "vue": "^3.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.6.6",
    "@vitejs/plugin-legacy": "^1.4.1",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.1",
    "eslint": "^7.28.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.34.1",
    "vite": "^2.3.7",
    "vite-plugin-live-reload": "^2.1.0"
  }
}
