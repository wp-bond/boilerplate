{
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "APP_ENV=production vite build",
    "dev-admin": "vite --config vite.config.wp-admin.js",
    "build-admin": "APP_ENV=production vite build --config vite.config.wp-admin.js",
    "build-editor": "vite build --config vite.config.wp-editor.js",
    "build-watch-editor": "vite build --config vite.config.wp-editor.js --watch",
    "build-all": "pnpm build && pnpm build-admin && pnpm build-editor",
    "upgrade": "pnpm dlx npm-check-updates -u && pnpm install"
  },
  "dependencies": {
    "gsap": "^3.10.4",
    "lodash-es": "^4.17.21",
    "ohmyfetch": "^0.4.16",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.0",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vue/compiler-sfc": "^3.2.33",
    "eslint": "^8.14.0",
    "postcss": "^8.4.13",
    "postcss-html": "^1.4.1",
    "postcss-preset-env": "^7.5.0",
    "stylelint": "^14.8.2",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vite-plugin-live-reload": "^2.1.0"
  }
}
