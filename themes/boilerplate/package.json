{
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "APP_ENV=production vite build",
    "dev-admin": "vite --config vite.config.wp-admin.js",
    "build-admin": "APP_ENV=production vite build --config vite.config.wp-admin.js",
    "build-editor": "vite build --config vite.config.wp-editor.js",
    "build-watch-editor": "vite build --config vite.config.wp-editor.js --watch",
    "build-all": "pnpm build && pnpm build-admin && pnpm build-editor",
    "upgrade": "pnpm dlx npm-check-updates -u && pnpm install"
  },
  "dependencies": {
    "gsap": "^3.11.4",
    "lodash-es": "^4.17.21",
    "ohmyfetch": "^0.4.21",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.45",
    "eslint": "^8.32.0",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^7.8.3",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-live-reload": "^3.0.1"
  }
}
